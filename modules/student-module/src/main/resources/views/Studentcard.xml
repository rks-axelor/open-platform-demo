<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
  
  <cards name="student-card" model="com.axelor.student.db.Student" title="Student">
            <field name="name"/>
		    <field name="rollNumber"/>
		    <field name="classNumber"/>
		     <field name="parentName"/>
		     <field name="schoolname"/>
		     <field name="address"/>
          <template>
		             <![CDATA[
		                <div class="span8 card-image">
		                <strong>{{name}}</strong>
		                <strong>{{rollNumber}}</strong>
		                <strong>{{classNumber}}</strong>
		                <strong>{{parentName}}</strong>
		                <strong>{{schoolname.schoolname}}</strong>
		                <strong>{{address.address}}</strong>
		           		</div>
		         	 ]]>    
          </template>
  
  </cards>
  
  </object-views>