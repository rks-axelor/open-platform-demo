<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
  
  <tree name="student-tree" title="Students">
		  <column name="studentname" type="string"/>
		  <column name="rollNumber" type="string"/>
		  <column name="classNumber" type="integer"/>
		  <column name="schoolname" type="reference"/>
		  <column name="address" type="reference"/>
	  <node model="com.axelor.student.db.Student">
	  
		  <field name="fullName" as="studentname"/>
		  <field name="rollNumber" as="rollNumber"/>
		  <field name="classNumber" as="classNumber"/>
		  <field name="schoolname" as="schoolname"/>
		  <field name="address" as="address"/>
     
     </node>
  </tree>
</object-views>